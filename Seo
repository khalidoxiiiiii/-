<script>/*<![CDATA[*/

let refh = window.location.origin.includes('seoplus');
        let currentRows = 3;
        let currentColumns = 3;

      function $getData(f) {
        document.querySelectorAll('.Getcodeform button').forEach((i)=>{
          i.addEventListener('click',()=>{

            let funct = (name)=>{
if(i.parentElement.querySelector('.ErrorMsg')){
   i.parentElement.querySelector('.ErrorMsg').innerHTML = '';
}
    let item = i.parentElement.querySelector(name);

  console.log(item.tagName)

     if(item.tagName.toLowerCase() == "input"){
      return item.value;
     }

     if(item.classList.contains('choosen')){
      return item.getAttribute('data-type');
     }

     if(item.tagName.toLowerCase() == "select"){
      return item.options[item.selectedIndex].value;
     }
            }

   let sharr = false;
            f(funct,i,sharr);

          })
        })
      };

/*]]>*/</script>
